{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^db:generate", "^ui:build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "type-check": {},
    "dev": {
      "dependsOn": ["^ui:build", "^db:generate"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "ui:build": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "web#build": {
      "dependsOn": ["^ui:build", "^db:generate"],
      "env": [
        "NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID",
        "NEXT_PUBLIC_BACKEND_ENDPOINT",
        "NEXT_PUBLIC_FRONTEND_URL",
        "NEXT_PUBLIC_ALCHEMY_GAS_MANAGER_POLICY_ID"
      ]
    },
    "web#dev": {
      "env": [
        "NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID",
        "NEXT_PUBLIC_BACKEND_ENDPOINT",
        "NEXT_PUBLIC_FRONTEND_URL",
        "NEXT_PUBLIC_ALCHEMY_GAS_MANAGER_POLICY_ID"
      ]
    }
  }
}
